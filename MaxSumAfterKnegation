#include <bits/stdc++.h>
using namespace std;

int main() {
	int a[6]={1,2,-3,0,-4,-1};
	int k=2;
	sort(a,a+6);
	for(int i=0;i<k;i++){
	    if(a[i]>=0){
	        continue;
	    }
	    else{
	        a[i]=-a[i];
	    }
	}
	int sum=0;
	for(int i=0;i<6;i++){
	    sum+=a[i];
	}
	cout<<sum<<endl;
	return 0;
}
