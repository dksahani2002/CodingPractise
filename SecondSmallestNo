#include<bits/stdc++.h>
using namespace std;
int main(){
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    int t;
    cin>>t;
    while(t--){
         
        set<int>st;
        set<int>:: iterator it;
        int n;
        cin>>n;
        for(int i=0;i<n;i++){
            int x;
            cin>>x;
            st.insert(x);
        }
        it=st.begin();
        it++;
        if(it!=st.end())
            cout<<(*it)<<"\n";
        else
            cout<<"-1\n";
    }
}
